{
  "name": "dom-native",
  "version": "0.11.5-WIP",
  "description": "A Native Web Component TypeScript Optimized Utility (zero dependency, < 20kb min, < 7kb gzip)",
  "author": "Jeremy Chone <jeremy.chone@gmail.com> (http://jeremychone.com)",
  "license": "MIT OR Apache-2.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "prepack": "npm run build",
    "build": "tsx --tsconfig scripts/tsconfig.json ./scripts/cmds.ts build",
    "build-dev-js": "./node_modules/.bin/rollup -c dev.rollup.config.js",
    "build-dev-css": "pcss",
    "build-lib": "tsx --tsconfig scripts/tsconfig.json scripts/cmds.ts build_lib",
    "watch": "tsx --tsconfig scripts/tsconfig.json ./scripts/cmds.ts watch"
  },
  "imports": {
    "#dom-native": "./src/index.js"
  },
  "devDependencies": {
    "@dom-native/demo-infra": "=0.0.0-internal-4",
    "@rollup/plugin-commonjs": "^29",
    "@rollup/plugin-node-resolve": "^16",
    "@rollup/plugin-typescript": "^12",
    "@types/d3-ease": "^3.0.2",
    "@types/lodash.debounce": "^4.0.9",
    "autoprefixer": "^10.4.19",
    "chokidar": "^5",
    "cmdrouter": "^0.3.1",
    "d3-ease": "^3.0.1",
    "execa": "^9.0.0",
    "fs-aux": "^0.2",
    "lodash.debounce": "^4.0.8",
    "postcss": "^8.4.38",
    "rollup": "^4.17.2",
    "terser": "^5.31.0",
    "tsx": "^4.19.2",
    "typescript": "^5.4.5",
    "utils-min": "^0.2.4",
    "pcss-cli": "^0.3"
  },
  "files": [
    "src/",
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/dom-native/dom-native.git"
  },
  "keywords": [
    "domnative",
    "dom native",
    "frameworkless",
    "framework",
    "Web Components",
    "Native Web Components",
    "customElements",
    "cssGrid",
    "DOM",
    "mvc",
    "html5"
  ]
}
