{
  "name": "dom-native",
  "version": "0.9.12",
  "description": "DOM Native micro library to build small to big web application (zero dependency, < 18kb min, < 8kb gzip)",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/dom-native/dom-native.git"
  },
  "keywords": [
    "web-framework",
    "WebComponent",
    "customElement",
    "cssGrid",
    "DOM",
    "DOM Native",
    "Patterns",
    "html",
    "framework",
    "mvc",
    "html5"
  ],
  "dependencies": {},
  "devDependencies": {
    "@dom-native/demo-infra": "0.0.0-internal-3",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash.debounce": "^4.0.6",
    "autoprefixer": "^9.8.6",
    "chokidar": "^3.4.2",
    "cloud-bucket": "^0.3.8",
    "cmdrouter": "^0.3.1",
    "fs-extra-plus": "^0.5.19",
    "js-yaml": "^3.14.0",
    "lodash.debounce": "^4.0.8",
    "p-spawn": "^0.5.4",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.2",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.3",
    "postcss-nested": "^4.2.3",
    "rollup": "^2.26.11",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "terser": "^5.3.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "utils-min": "^0.1.16"
  },
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "prepack": "npm run build",
    "build": "TS_NODE_FILES=true ./node_modules/.bin/ts-node --project scripts/tsconfig.json  scripts/cmds.ts build",
    "build-dev-js": "./node_modules/.bin/rollup -c dev.rollup.config.js",
    "build-dev-css": "./node_modules/.bin/postcss --config ./demo/ -o demo/dist/demo-bundle.css  demo/pcss/main.pcss",
    "build-lib": "TS_NODE_FILES=true ./node_modules/.bin/ts-node --project scripts/tsconfig.json  scripts/cmds.ts build_lib",
    "watch": "TS_NODE_FILES=true ./node_modules/.bin/ts-node --project scripts/tsconfig.json  scripts/cmds.ts watch",
    "site": "TS_NODE_FILES=true ./node_modules/.bin/ts-node --project scripts/tsconfig.json  scripts/cmds.ts site",
    "cdn": "TS_NODE_FILES=true ./node_modules/.bin/ts-node --project scripts/tsconfig.json  scripts/cmds.ts cdn"
  },
  "author": "Jeremy Chone <jeremy.chone@gmail.com> (http://jeremychone.com)",
  "license": "MIT"
}
