{
  "name": "dom-native",
  "version": "0.9.17-SNAPSHOT",
  "description": "Small set of utilities for building small to big native Web Component based applications (zero dependency, < 18kb min, < 7kb gzip)",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/dom-native/dom-native.git"
  },
  "keywords": [
    "web-framework",
    "WebComponent",
    "customElement",
    "cssGrid",
    "DOM",
    "DOM Native",
    "Patterns",
    "html",
    "framework",
    "mvc",
    "html5"
  ],
  "dependencies": {},
  "devDependencies": {
    "@dom-native/demo-infra": "0.0.0-internal-3",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@types/d3-ease": "^2.0.0",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash.debounce": "^4.0.6",
    "autoprefixer": "^10.0.2",
    "chokidar": "^3.4.3",
    "cloud-bucket": "^0.3.10",
    "cmdrouter": "^0.3.1",
    "d3-ease": "^2.0.0",
    "fs-extra-plus": "^0.5.20",
    "js-yaml": "^3.14.0",
    "lodash.debounce": "^4.0.8",
    "p-spawn": "^0.5.5",
    "postcss": "^8.1.7",
    "postcss-cli": "^8.2.0",
    "postcss-import": "^13.0.0",
    "postcss-nested": "^5.0.1",
    "rollup": "^2.33.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "terser": "^5.3.8",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "utils-min": "^0.1.17"
  },
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "prepack": "npm run build",
    "build": "TS_NODE_FILES=true ./node_modules/.bin/ts-node --project scripts/tsconfig.json  scripts/cmds.ts build",
    "build-dev-js": "./node_modules/.bin/rollup -c dev.rollup.config.js",
    "build-dev-css": "./node_modules/.bin/postcss --config ./demo/ -o demo/dist/demo-bundle.css  demo/pcss/main.pcss",
    "build-lib": "TS_NODE_FILES=true ./node_modules/.bin/ts-node --project scripts/tsconfig.json  scripts/cmds.ts build_lib",
    "watch": "TS_NODE_FILES=true ./node_modules/.bin/ts-node --project scripts/tsconfig.json  scripts/cmds.ts watch",
    "site": "TS_NODE_FILES=true ./node_modules/.bin/ts-node --project scripts/tsconfig.json  scripts/cmds.ts site",
    "cdn": "TS_NODE_FILES=true ./node_modules/.bin/ts-node --project scripts/tsconfig.json  scripts/cmds.ts cdn"
  },
  "author": "Jeremy Chone <jeremy.chone@gmail.com> (http://jeremychone.com)",
  "license": "MIT"
}
