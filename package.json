{
  "name": "dom-native",
  "version": "0.9.10-SNAPSHOT",
  "description": "DOM Native micro library to build small to big web application (zero dependency, < 12kb min, < 5kb gzip)",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/dom-native/dom-native.git"
  },
  "keywords": [
    "web-framework",
    "WebComponent",
    "customElement",
    "cssGrid",
    "DOM",
    "DOM Native",
    "Patterns",
    "html",
    "framework",
    "mvc",
    "html5"
  ],
  "dependencies": {},
  "devDependencies": {
    "@dom-native/demo-infra": "0.0.0-internal-2",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@types/lodash.debounce": "^4.0.6",
    "autoprefixer": "^9.8.6",
    "chokidar": "^3.4.2",
    "cmdrouter": "^0.3.1",
    "fs-extra-plus": "^0.5.19",
    "lodash.debounce": "^4.0.8",
    "postcss": "^7.0.32",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.3",
    "postcss-nested": "^4.2.3",
    "rollup": "^2.26.9",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "terser": "^5.2.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "utils-min": "^0.1.16",
    "vdev": "^0.12.3"
  },
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "prepare": "rm -Rf ./dist && ./node_modules/.bin/tsc",
    "test": "TS_NODE_FILES=true ./node_modules/.bin/ts-node --project scripts/tsconfig.json  scripts/src/cmds.ts test",
    "build": "TS_NODE_FILES=true ./node_modules/.bin/ts-node --project scripts/tsconfig.json  scripts/src/cmds.ts build",
    "watch": "TS_NODE_FILES=true ./node_modules/.bin/ts-node --project scripts/tsconfig.json  scripts/src/cmds.ts watch",
    "site": "./node_modules/.bin/vdev sup demo/ jc-sites:dom-native/demo/core/"
  },
  "author": "Jeremy Chone <jeremy.chone@gmail.com> (http://jeremychone.com)",
  "license": "MIT"
}
